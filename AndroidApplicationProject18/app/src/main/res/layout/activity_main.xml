        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RelativeLayout001"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_recycler_view005" />

    <Button
        android:id="@+id/btn_table_layout_test001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TableLayout001"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_relative_layout_test001"
        app:layout_constraintTop_toBottomOf="@+id/btn_recycler_view005" />

    <Button
        android:id="@+id/btn_smart_refresh_layout_test001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SmartRefreshLayout001"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_relative_layout_test001" />
</androidx.constraintlayout.widget.ConstraintLayout>