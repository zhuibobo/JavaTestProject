    <Button
        android:id="@+id/btn_swipe_refresh_view002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SwipeRefreshLayoutTest002"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_swipe_refresh_view001"
