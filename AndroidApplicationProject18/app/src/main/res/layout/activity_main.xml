        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/btn_swipe_refresh_view001" />

    <Button
        android:id="@+id/btn_recycler_view002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
